{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/main/cli/schemas/config-file.v1.json",
  "name": "@nyoon/vite-plugin-inline",
  "version": "0.1.5",
  "exports": "./mod.ts",
  "lint": {
    "rules": {
      "include": [
        "default-param-last",
        "eqeqeq",
        "explicit-module-boundary-types",
        "guard-for-in",
        "no-eval",
        "no-inferrable-types",
        "no-self-compare",
        "no-sparse-arrays",
        "no-useless-rename",
        "prefer-ascii",
        "triple-slash-reference",
        "verbatim-module-syntax"
      ],
      "exclude": ["ban-types", "no-cond-assign", "no-explicit-any"]
    }
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.42.3"
  },
  "tasks": {
    "build": "deno run --allow-all ./build.ts"
  }
}
